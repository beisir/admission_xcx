.contactCon1{position: relative; height: 120rpx;}
.clRig{position: absolute; left: 270rpx;top:35rpx; line-height: 45rpx;right:20rpx;}
.contacts{font-size: 34rpx; color: #333;}
.contacts text{color: #666; font-size: 24rpx;padding-left: 10rpx;}
.conName{font-size: 28rpx; color:#666;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
/* .mapBtn{margin-bottom:80rpx;}*/
.mapBtn {
    position: fixed;
    bottom: 10px;
    right: 10px;
}
/* position:fixed;right:30rpx;bottom:120rpx; */
.mapBtn .btn-map{height:90rpx;width:90rpx;border-radius:50%; border: none;background:#fd6565; color: #fff; font-size:24rpx; text-align: center; padding:16rpx 12rpx; line-height:30rpx; box-shadow:0 0 30rpx #ffbebe;z-index: 50000;}
.btn-map cover-view{
    margin-top:10rpx;
    font-size:14px;
}






.botFixed, .botFixed2, .botFixed3 {
    position: fixed;
    height: 98rpx;
    left: 0;
    bottom: 0;
    width: 100%;
    background: #fff;
    box-shadow: 0 1px 5rpx 4rpx #ddd;
    font-size: 24rpx;
    color: #666;
    text-align: center;
}

.classLink, .telLink, .companyLink {
    width: 20%;
    float: left;
    padding-top: 12rpx;
    height: 86rpx;
}

.supplierBtn {
    width: 40%;
    float: left;
    font-size: 32rpx;
    line-height: 98rpx;
    background: #fd5555;
    text-align: center;
    color: #fff;
}

.classLink:before, .telLink:before, .companyLink:before {
    content: "";
    display: block;
    width: 50rpx;
    height: 45rpx;
    margin: 0 auto 2rpx;
    background: url(https://style.org.hc360.com/images/admission_xcx/icon.png) no-repeat;
    background-size: 300rpx 600rpx;
}

.classLink::before {
    background-position: 5rpx -95rpx;
}

.telLink::before {
    background-position: -43rpx -95rpx;
}

.companyLink::before {
    background-position: -93rpx -95rpx;
}





