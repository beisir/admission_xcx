.mallHeader{height:100rpx; background:url(https://style.org.hc360.com/images/admission_xcx/mallTopBg.png) bottom; background-size:750rpx 190rpx; padding:45rpx 30rpx; position: relative;}
.logoBox{width:100rpx;height:100rpx; float: left; border-radius: 8rpx;}
.logoBox image{height:100rpx;width:100rpx;}
.logoRig{float: left;padding-top:2rpx;width:calc (100% - 300rpx);}
.mallName{color: #fff;font-size:30rpx;line-height: 50rpx;-o-text-overflow:ellipsis;text-overflow:ellipsis;overflow:hidden;white-space:nowrap;}
.mallMmt{font-size: 26rpx;color: #fff; line-height: 40rpx;}
.mallMmt icon{ display: block; float: left; width:35rpx;height: 32rpx; margin-right: 5rpx;background:url(https://style.org.hc360.com/images/admission_xcx/icon.png) -220rpx 1rpx no-repeat; background-size:300rpx 600rpx; margin-top: 5rpx;}
.mallMmt text{ margin-left:15rpx;}
.logoRig{position:absolute; left:150rpx;right:30rpx;top:45rpx;}
.navBox{/*overflow:hidden;*/height:113rpx; background: #fff;}
.navLink{width:25%; padding-top:15rpx;float: left; height:97rpx;border-bottom: 1rpx solid #e5e5e5; text-align: center;font-size: 24rpx; color: #333;}
.navLink:before{ content:""; display: block; width: 55rpx;height:50rpx; margin: 0 auto 4rpx;background:url(https://style.org.hc360.com/images/admission_xcx/icon.png) no-repeat; background-size:300rpx 600rpx;}
.navLink:nth-child(1)::before{ background-position:5rpx 5rpx;}
.navLink:nth-child(2)::before{ background-position:-46rpx 5rpx;}
.navLink:nth-child(3)::before{ background-position:-103rpx 5rpx;}
.navLink:nth-child(4)::before{ background-position:-161rpx 5rpx;}
.navLink.cur:nth-child(1)::before{ background-position:5rpx -45rpx;}
.navLink.cur:nth-child(2)::before{ background-position:-46rpx -45rpx;}
.navLink.cur:nth-child(3)::before{ background-position:-103rpx -45rpx;}
.navLink.cur:nth-child(4)::before{ background-position:-161rpx -45rpx;}
.navLink.cur{color:#fd5555; border-bottom:4rpx solid #fd5555; height:95rpx}





