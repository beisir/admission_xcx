/* pages/setup_shop/setup_shop.wxss */
page{ background:#f5f5f5;}
.setUpShopTop{overflow:hidden; padding:40rpx 0 5rpx; }
.setUpShopTop icon{height:120rpx;width:120rpx;background:url(https://style.org.hc360.com/images/admission_xcx/indexIcon.png) 0 -680rpx no-repeat;background-size:210rpx 800rpx; margin:0 auto; display: block;}
.setUpShopTop text{font-size:28rpx;color:#333;line-height:66rpx;display:block;text-align:center;}
.setUpShopBot{padding:30rpx 35rpx;}
.setUpShopCon{overflow:hidden; background: #fff; border-radius: 15rpx;}
.setUpShopList{overflow:hidden;height:90rpx; line-height:90rpx; position: relative;}
.setUpShopList text{display: block; float: left;width:150rpx; text-align: right; font-size: 28rpx; color: #333;line-height:90rpx;}
.setUpShopList icon{ display: block;height:50rpx;width:50rpx; position:absolute; top: 20rpx;left: 25rpx;background:url(https://style.org.hc360.com/images/admission_xcx/indexIcon.png) no-repeat;background-size:210rpx 800rpx;}
.setUpShopRig{height:88rpx; border-bottom:1rpx solid #f0f0f0; position: absolute; top: 0; right:0;left:85rpx; overflow: hidden;}
.setUpShopRig input{width: 100%;height:90rpx; color: #333; font-size: 28rpx;}
.setUpShopBot button{height:84rpx; font-size: 34rpx; color: #fff; background:#fd5555; border: 0; border-radius:60rpx; margin: 70rpx 0 30rpx;}
.setUpShopList icon.sIcon1{background-position:0 -76rpx;}
.setUpShopList icon.sIcon2{background-position:-55rpx -78rpx;}
.setUpShopList icon.sIcon3{background-position:-110rpx -78rpx;}
.setUpShopList icon.sIcon4{background-position:-153rpx -78rpx;}
.setUpShopList icon.sIcon5{background-position:-167rpx -633rpx;}
.setUpShopList icon.sIcon6{background-position:-167rpx -700rpx;}
.setUpShopList button{position: absolute;right: 0;top: 0;height:90rpx; color: #fff; font-size: 28rpx;width:220rpx; margin: 0; background: #fd5555; line-height: 90rpx; border:none; border-radius: 0; z-index: 50;}
.setUpShopList button.grayBtn{color: #999;background: #f0f0f0; z-index: 51;}
button::after{content:""; border: 0;}
.agreementLink{ line-break: 80rpx; font-size: 24rpx; color: #999}
.agreementLink navigator{color:#fd5555;display: inline;}
.codeBox{height:70rpx;position: absolute;right:0rpx;top:10rpx;width:220rpx; z-index: 100;}
.codeBox image{width:220rpx;height:70rpx;}